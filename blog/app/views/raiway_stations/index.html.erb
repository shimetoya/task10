<p id="notice"><%= notice %></p>

<h1>Listing Raiway Stations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @raiway_stations.each do |raiway_station| %>
      <tr>
        <td><%= raiway_station.title %></td>
        <td><%= link_to 'Show', raiway_station %></td>
        <td><%= link_to 'Edit', edit_raiway_station_path(raiway_station) %></td>
        <td><%= link_to 'Destroy', raiway_station, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Raiway station', new_raiway_station_path %>
